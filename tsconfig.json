{
    "compilerOptions": {
        "target": "ES2017",
        "module": "ESNext",
        "moduleResolution": "node",
        "outDir": "./Templates/js",
        "rootDir": "./src/Frontend/ts",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src/Frontend/ts/**/*"
    ]
}
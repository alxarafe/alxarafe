{
  "name": "alxarafe/alxarafe",
  "type": "library",
  "minimum-stability": "stable",
  "description": "Alxarafe. Development of PHP applications in a flash!",
  "keywords": [
    "php",
    "libraries",
    "helpers"
  ],
  "homepage": "https://github.com/alxarafe/alxarafe",
  "license": "MIT",
  "authors": [
    {
      "name": "Alxarafe",
      "email": "info@alxarafe.com",
      "homepage": "https://alxarafe.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "symfony/yaml": "^4.2",
    "monolog/monolog": "^1.24",
    "maximebf/debugbar": "^1.0",
    "twig/twig": "^2.0",
    "symfony/translation": "^4.2",
    "ext-pdo": "*",
    "ext-openssl": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "symfony/finder": "^4.2",
    "symfony/cache": "^4.2",
    "parsecsv/php-parsecsv": "^1.1",
    "aura/session": "^2.1",
    "kint-php/kint": "^3.1",
    "symfony/http-foundation": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "Alxarafe\\": "src/Alxarafe"
    },
    "files": [
      "src/Alxarafe/Core/Functions.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.4",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "scripts": {
    "post-update-cmd": [
      "bower update"
    ],
    "post-install-cmd": [
      "bower install"
    ]
  }
}
